generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Post {
  id        String   @id @default(cuid())
  content   String
  mood      String
  moodColor String
  sessionId String   // ðŸ‘ˆ YE ADD KIA
  createdAt DateTime @default(now())
  expiresAt DateTime
}
