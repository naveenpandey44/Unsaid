generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Post {
  id        String   @id @default(cuid())
  content   String
  mood      String
  moodColor String
  sessionId String // ðŸ‘ˆ YE ADD KIA
  createdAt DateTime @default(now())
  expiresAt DateTime
}
